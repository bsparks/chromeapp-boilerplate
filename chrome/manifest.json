{
  "name": "ChromeApp Experiment",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["app.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage",
    "unlimitedStorage",
    "webview"
  ]
}
